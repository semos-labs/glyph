{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "packages/glyph/src/index.ts",
    "packages/glyph-markdown/src/index.ts"
  ],
  "tsconfig": "tsconfig.docs.json",
  "plugin": ["typedoc-plugin-markdown"],

  "out": ".docs-out",
  "cleanOutputDir": true,

  "name": "@semos-labs/glyph",
  "readme": "none",
  "entryFileName": "index",
  "disableSources": true,
  "excludeExternals": true,
  "excludeInternal": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeNotDocumented": false,
  "externalSymbolLinkMappings": {
    "@semos-labs/glyph": {
      "GlyphNode": "#",
      "UseAppResult": "#",
      "UseFocusResult": "#"
    },
    "@semos-labs/glyph-markdown": {
      "Highlighter": "#",
      "CreateHighlighterOptions": "#"
    }
  },
  "groupOrder": [
    "Layout",
    "Tables",
    "Form",
    "Navigation",
    "Keybindings",
    "Feedback",
    "Markdown",
    "Core",
    "Type Aliases",
    "Interfaces",
    "Functions",
    "*"
  ],
  "categoryOrder": [
    "Layout",
    "Tables",
    "Form",
    "Navigation",
    "Keybindings",
    "Feedback",
    "Markdown",
    "Core",
    "*"
  ],
  "hidePageHeader": false,
  "hidePageTitle": false,
  "hideBreadcrumbs": true,
  "useCodeBlocks": true,
  "expandObjects": true,
  "parametersFormat": "table",
  "propertiesFormat": "table",
  "enumMembersFormat": "table",
  "typeDeclarationFormat": "table",
  "indexFormat": "table",
  "outputFileStrategy": "members",
  "flattenOutputFiles": false,
  "fileExtension": ".md"
}
